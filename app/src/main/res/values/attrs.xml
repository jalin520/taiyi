<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="SettingView">
        <attr name="text" format="string"/>
        <attr name="show_top_line_full" format="boolean"></attr>
        <attr name="show_top_line_wrap" format="boolean"></attr>
        <attr name="show_bottom_line_full" format="boolean"></attr>
        <attr name="src" format="reference"></attr>
    </declare-styleable>

    <declare-styleable name="MessageView">
        <attr name="messageTitle" format="string"></attr>
        <attr name="content" format="string"></attr>
        <attr name="time" format="string"></attr>
        <attr name="messageIcon" format="reference"></attr>
    </declare-styleable>


    <declare-styleable name="ScoreView">
        <attr name="circle_width" format="dimension"></attr>
        <attr name="circle_height" format="dimension"></attr>
        <attr name="circle_text_size" format="dimension"></attr>
        <attr name="circle_score" format="integer"></attr>
    </declare-styleable>

    <declare-styleable name="ComparInfoView">
        <attr name="mTitle" format="string"></attr>
        <attr name="mLeftName" format="string"></attr>
        <attr name="mRightName" format="string"></attr>
    </declare-styleable>

    <declare-styleable name="ClassFictionView">
        <attr name="mTv" format="string"/>
        <attr name="mImageView" format="reference"/>
        <attr name="isVisible" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SupposeYouWantView">
        <attr name="SupposeYouWantViewTitle" format="string" />

    </declare-styleable>


    <attr name="borderRadius" format="dimension"/>
    <attr name="type">
        <enum name="circle" value="0"/>
        <enum name="round" value="1"/>
    </attr>
    <attr name="srcImage" format="reference" />

    <declare-styleable name="CustomImageView">
        <attr name="borderRadius"/>
        <attr name="type"/>
        <attr name="srcImage"/>
    </declare-styleable>

    <declare-styleable name="WaterWaveProgress">
        <attr name="progressWidth" format="dimension" />
        <attr name="fontSize" format="dimension" />
        <attr name="progressColor" format="color" />
        <attr name="progressBgColor" format="color" />
        <attr name="waterWaveColor" format="color" />
        <attr name="waterWaveBgColor" format="color" />
        <attr name="progress2WaterWidth" format="dimension" />
        <attr name="showProgress" format="boolean" />
        <attr name="showNumerical" format="boolean" />
        <attr name="textColor" format="color" />
        <attr name="progress" format="integer" />
        <attr name="maxProgress" format="integer" />
    </declare-styleable>

</resources>